bitcoind: ./$BITCOIN_BRANCH_DIR/src/$BITCOIN_DAEMON -rpcbind=$BITCOIN_HOST:$BITCOIN_PORT -datadir=$BITCOIN_DATADIR -rpcallowip=$BITCOIN_ALLLOWFROM -printtoconsole -logips -logtimestamps -server -txindex -rpcuser=$BITCOIN_RPCUSER -rpcpassword=$BITCOIN_RPCPASSWORD  -listen -discover -persistmempool -dbcache=8000 -zmqpubhashblock=$BITCOIN_ZMQ

testnet3: ./$BITCOIN_BRANCH_DIR/src/$BITCOIN_DAEMON -rpcbind=$TESTNET3_HOST:$TESTNET3_PORT -datadir=$TESTNET3_DATADIR -rpcallowip=$TESTNET3_ALLLOWFROM -testnet -printtoconsole -logips -logtimestamps -server -txindex -rpcuser=$TESTNET3_RPCUSER -rpcpassword=$TESTNET3_RPCPASSWORD  -listen -discover -persistmempool -dbcache=8000 -zmqpubhashblock=$TESTNET3_ZMQ
